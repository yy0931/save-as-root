{
	"name": "save-as-root",
	"displayName": "Save as Root in Remote - SSH",
	"description": "Edit configuration files with root privileges during using the Remote - SSH extension",
	"version": "1.4.2",
	"publisher": "yy0931",
	"engines": {
		"vscode": "^1.57.0"
	},
	"repository": {
		"url": "https://github.com/yy0931/save-as-root",
		"type": "git"
	},
	"bugs": {
		"url": "https://github.com/yy0931/save-as-root/issues"
	},
	"icon": "icon.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"onCommand:save-as-root.saveFile"
	],
	"keywords": [
		"remote",
		"ssh",
		"root",
		"sudo"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "save-as-root.saveFile.noconfirm",
				"title": "Save as Root"
			},
			{
				"command": "save-as-root.saveFile",
				"title": "Save as Root"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "save-as-root.saveFile.noconfirm"
				},
				{
					"command": "save-as-root.saveFile",
					"when": "false"
				}
			]
		}
	},
	"capabilities": {
		"virtualWorkspaces": false,
		"untrustedWorkspaces": {
			"supported": true
		}
	}
}
